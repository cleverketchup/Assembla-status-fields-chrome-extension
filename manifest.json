{
  "manifest_version": 2,
  "name": "Assembla status fields",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.assembla.com/spaces/*"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "content.js"
      ],
      "css": ["styles.css"]
    }
  ]
}